          <li>Free To Play</li>
        </ul>
      </nav>
    </header>
    <main>

      
      <div class="container">
        
        <% games.forEach(game => { %>
          
          <section class="card">
          <a  href=<%= game.game_url %> target="_blank">
              <h3 class="card-title"><%= game.title %></h3>
              <img clas="card-img" src=<%= game.thumbnail %> alt=<%= game.title %> />
              <p class="card-platform"><%= game.platform %></p>
            </a>
          </section>

        <% }) %>
        

      </div>
      
    </main>
